{
  "name": "user",
  "plural": "users",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "emailOptional": true
  },
  "properties": {
    "email": {
      "type": "string",
      "required": false
    },
    "username": {
      "type": "string",
      "required": true,
      "index": {"unique": true}
    },
    "address": {
      "type": "string",
      "index": {"unique": true}
    },
    "balance": {
      "type": "number",
      "default": 0,
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "groups": {
      "type": "hasAndBelongsToMany",
      "model": "Group",
      "foreignKey": ""
    },
    "goals": {
      "type": "hasMany",
      "model": "Goal",
      "foreignKey": "",
      "through": "Group"
    },
    "transactions": {
      "type": "hasMany",
      "model": "Transaction",
      "foreignKey": ""
    },
    "activities": {
      "type": "hasMany",
      "model": "Activity",
      "foreignKey": ""
    }
  },
  "mixins": {
    "Readonly" : {
      "address" : true,
      "balance" : true
    }
  },
  "acls": [],
  "methods": {}
}
