{
  "name": "Transaction",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "value": {
      "type": "number",
      "required": true
    },
    "hash": {
      "type": "string",
      "required": true,
      "index": {"unique": true}
    },
    "from_address": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "date",
      "defaultFn": "now",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
